  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^5.0.4",
    "@types/node": "^10.12.15",
    "@types/shelljs": "^0.8.1",
    "fs-extra": "^7.0.1",
    "nodemon": "^1.18.8",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "shelljs": "^0.8.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
